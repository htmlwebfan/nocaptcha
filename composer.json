{
    "name": "htmlwebfan/nocaptcha",
    "description": "Generates and verifies a NoCaptcha field in forms HTML forms with PHP.",
    "keywords": ["nocaptcha, html, form, robot, spam, turing test, recaptcha, spamprevention, antispam"],
    "homepage": "https://github.com/htmlwebfan/nocaptcha",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Way",
            "homepage": "http://www.htmlwebfan.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "autoload": {
        "psr-4": {
            "HtmlWebFan\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HtmlWebFan\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
